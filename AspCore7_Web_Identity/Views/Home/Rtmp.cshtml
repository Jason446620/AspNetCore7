@{
    ViewData["Title"] = "Rtmp";
}
<!DOCTYPE html>
<html>
<head>
	<title>臭周芮</title>
    <meta name="referrer" content="origin">
</head>
<body>

    <script src="http://bilibili.github.io/flv.js/dist/flv.js"></script>
	<div class="video-container">
            <div>
                <video name="videoElement" id="video" class="centeredVideo" controls autoplay>
                    Your browser is too old which doesn't support HTML5 video.
                </video>
            </div>
        </div>
	<script>
        var flvPlayer;
        if (flvjs.isSupported()) {
            var video = document.getElementById('video');
            video.autoplay = false;
            flvPlayer = flvjs.createPlayer({
                type: 'flv',
                isLive: true,
                hasAudio: false,
                hasVideo: true,
                url: 'http://192.168.1.3:8080/live/livestream.flv',//<==自行修改

            });
            flvPlayer.attachMediaElement(video);
            flvPlayer.load();
            flvPlayer.play();
        }
        function flv_start() {
            flvPlayer.load(); //加载
        }


	</script>
</body>
</html>
